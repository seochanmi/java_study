package test.main;

import java.util.Scanner;

import test.mypac.Phone;

/*
 * 	java 에서 사용하는 모든 data 는 object type 변수나 필드에 담을수 있다.
 */
public class MainClass07 {
	public static void main(String[] args) {
		//기본 data type은 Wrapper Class type으로 간주되어서 담긴다.
		Object a = 10; //Integer
		Object b =true; //Boolean
		Object c = 'a'; //Character
		//모든 참조 data type 은 당연히 Object type도 된다.
		Object d = "abcd";
		Object e = new Phone();
		Object f = new Scanner(System.in);
	}

}
